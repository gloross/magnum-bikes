<section class="premium-high" style="background: #fff;">
  <div class="container container--small">
    <h3>Reviews</h3>
    {% for block in section.blocks %}
      {% render block %}
    {% endfor %}
  </div>
</section>
{% schema %}
{
  "name": "t:sections.apps.name",
  "settings": [
    {
      "type": "checkbox",
      "id": "include_container",
      "default": true,
      "label": "t:sections.apps.settings.include_container.label"
    }
  ],
  "blocks": [
    {
      "type": "@app"
    }
  ],
  "presets": [
    {
      "name": "t:sections.apps.presets.name"
    }
  ]
}
{% endschema %}
